    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fontkit": "^2.0.4",
    "open": "^10.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "xlsx": "^0.18.5",
    "yoga-layout-prebuilt": "^1.10.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.5",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6"
  }
}
